import 'package:pb_dtos/pb/dto/dto.dart';
import 'package:pb_dtos/pb/dto/dto_field.dart';
import 'friends_dto.dart';
import 'users_dto_field_select.dart';
import 'permissions_dto_field_select.dart';
import 'roles_dto_field_select.dart';
import 'follows_dto_field_select.dart';
import 'friends_dto_field_select.dart';
import 'blocks_dto_field_select.dart';
import 'posts_dto_field_select.dart';
import 'friends_dto_expand.dart';

class FriendsDtoFieldSelect<D extends Dto<D>>
    extends DtoFieldSelectBase<FriendsDto> {
  FriendsDtoFieldSelect() : super();

  FriendsDtoFieldSelect.nested(super.$parts, super.$fieldChain)
    : super.nested();

  void requester() => $addField(FriendsDtoFieldEnum.requester);

  void accepter() => $addField(FriendsDtoFieldEnum.accepter);

  void state() => $addField(FriendsDtoFieldEnum.state);

  ModifiableStringField get id => $addModifiableField(FriendsDtoFieldEnum.id);

  FriendsDtoExpandDtoFieldSelect<FriendsDto> get expand =>
      $addExpand<FriendsDto, FriendsDtoExpandDtoFieldSelect<FriendsDto>>(
        FriendsDtoExpandDtoFieldSelect.new,
      );
}

class FriendsDtoExpandDtoFieldSelect<D extends Dto<D>>
    extends DtoFieldSelectBase<FriendsDto> {
  FriendsDtoExpandDtoFieldSelect(super.$parts, super.$fieldChain)
    : super.nested();

  UsersDtoFieldSelect<FriendsDto> get requester =>
      $addRelation(UsersDtoFieldSelect.nested, FriendsDtoFieldEnum.requester);

  UsersDtoFieldSelect<FriendsDto> get accepter =>
      $addRelation(UsersDtoFieldSelect.nested, FriendsDtoFieldEnum.accepter);
}
