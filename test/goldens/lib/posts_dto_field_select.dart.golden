import 'package:pb_dtos/pb/dto/dto.dart';
import 'package:pb_dtos/pb/dto/dto_field.dart';
import 'posts_dto.dart';
import 'users_dto_field_select.dart';
import 'permissions_dto_field_select.dart';
import 'roles_dto_field_select.dart';
import 'follows_dto_field_select.dart';
import 'friends_dto_field_select.dart';
import 'blocks_dto_field_select.dart';
import 'posts_dto_field_select.dart';
import 'posts_dto_expand.dart';

class PostsDtoFieldSelect<D extends Dto<D>>
    extends DtoFieldSelectBase<PostsDto> {
  PostsDtoFieldSelect() : super();

  PostsDtoFieldSelect.nested(super.$parts, super.$fieldChain) : super.nested();

  void poster() => $addField(PostsDtoFieldEnum.poster);

  ModifiableStringField get message =>
      $addModifiableField(PostsDtoFieldEnum.message);

  void photo() => $addField(PostsDtoFieldEnum.photo);

  void link() => $addField(PostsDtoFieldEnum.link);

  void location() => $addField(PostsDtoFieldEnum.location);

  void reviewStars() => $addField(PostsDtoFieldEnum.reviewStars);

  void tagged() => $addField(PostsDtoFieldEnum.tagged);

  void draft() => $addField(PostsDtoFieldEnum.draft);

  void scheduled() => $addField(PostsDtoFieldEnum.scheduled);

  void visibility() => $addField(PostsDtoFieldEnum.visibility);

  void created() => $addField(PostsDtoFieldEnum.created);

  void metadata() => $addField(PostsDtoFieldEnum.metadata);

  ModifiableStringField get id => $addModifiableField(PostsDtoFieldEnum.id);

  PostsDtoExpandDtoFieldSelect<PostsDto> get expand =>
      $addExpand<PostsDto, PostsDtoExpandDtoFieldSelect<PostsDto>>(
        PostsDtoExpandDtoFieldSelect.new,
      );
}

class PostsDtoExpandDtoFieldSelect<D extends Dto<D>>
    extends DtoFieldSelectBase<PostsDto> {
  PostsDtoExpandDtoFieldSelect(super.$parts, super.$fieldChain)
    : super.nested();

  UsersDtoFieldSelect<PostsDto> get poster =>
      $addRelation(UsersDtoFieldSelect.nested, PostsDtoFieldEnum.poster);

  UsersDtoFieldSelect<PostsDto> get tagged =>
      $addRelation(UsersDtoFieldSelect.nested, PostsDtoFieldEnum.tagged);
}
