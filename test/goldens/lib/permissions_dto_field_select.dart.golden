import 'package:pb_dtos/pb/dto/dto.dart';
import 'package:pb_dtos/pb/dto/dto_field.dart';
import 'permissions_dto.dart';
import 'users_dto_field_select.dart';
import 'permissions_dto_field_select.dart';
import 'roles_dto_field_select.dart';
import 'follows_dto_field_select.dart';
import 'friends_dto_field_select.dart';
import 'blocks_dto_field_select.dart';
import 'posts_dto_field_select.dart';
import 'private_profiles_dto_field_select.dart';
import 'permissions_dto_expand.dart';

class PermissionsDtoFieldSelect<D extends Dto<D>>
    extends DtoFieldSelectBase<PermissionsDto> {
  PermissionsDtoFieldSelect() : super();

  PermissionsDtoFieldSelect.nested(super.$parts, super.$fieldChain)
    : super.nested();

  ModifiableStringField get name =>
      $addModifiableField(PermissionsDtoFieldEnum.name_);

  ModifiableStringField get id =>
      $addModifiableField(PermissionsDtoFieldEnum.id);

  PermissionsDtoExpandDtoFieldSelect<PermissionsDto> get expand =>
      $addExpand<
        PermissionsDto,
        PermissionsDtoExpandDtoFieldSelect<PermissionsDto>
      >(PermissionsDtoExpandDtoFieldSelect.new);
}

class PermissionsDtoExpandDtoFieldSelect<D extends Dto<D>>
    extends DtoFieldSelectBase<PermissionsDto> {
  PermissionsDtoExpandDtoFieldSelect(super.$parts, super.$fieldChain)
    : super.nested();

  RolesDtoFieldSelect<PermissionsDto> get rolesViaPermissions => $addRelation(
    RolesDtoFieldSelect.nested,
    PermissionsDtoExpandEnum.rolesViaPermissions,
  );
}
