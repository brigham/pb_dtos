enum PermissionsDtoFieldEnum<V> implements DtoTypedField<PermissionsDto, V> {
  name_<String>('name'),
  id<String>('id');

  const PermissionsDtoFieldEnum(this.pbName);

  @override
  final String pbName;
}

@freezed
@JsonSerializable(explicitToJson: true, includeIfNull: false)
class PermissionsDto with _$PermissionsDto implements Dto<PermissionsDto> {
  static PermissionsDtoMeta meta() => const PermissionsDtoMeta();

  static PermissionsDtoSort<PermissionsDto> sort(
    void Function(PermissionsDtoSort<PermissionsDto>) builder,
  ) {
    var sort = PermissionsDtoSort<PermissionsDto>();
    builder(sort);
    return sort;
  }

  static PermissionsDtoFilter filter(
    void Function(PermissionsDtoFilter) builder,
  ) {
    var filter = PermissionsDtoFilter();
    builder(filter);
    return filter;
  }

  static PermissionsDtoFilter filterByIdxPermissionsName(String name) {
    final f = PermissionsDtoFilter();
    f.name().equal(name);
    return f;
  }

  static PermissionsDtoSort<PermissionsDto> sortByIdxPermissionsName() {
    final s = PermissionsDtoSort<PermissionsDto>();
    s.name();
    return s;
  }

  static PermissionsPatchDto patch(void Function(PermissionsPatchDto) builder) {
    var patch = PermissionsPatchDto();
    builder(patch);
    return patch;
  }

  @override
  PermissionsPatchDto asPatch() => PermissionsPatchDto()..name = name;

  @override
  PermissionsPatchDto diff(PermissionsDto newValue) =>
      PermissionsPatchDto()
        ..name = name != newValue.name ? newValue.name : null;

  static PermissionsDtoExpand<PermissionsDto> expansions(
    void Function(PermissionsDtoExpand) builder,
  ) {
    var expansions = PermissionsDtoExpand<PermissionsDto>();
    builder(expansions);
    return expansions;
  }

  static PermissionsDtoFieldSelect<PermissionsDto> fields(
    void Function(PermissionsDtoFieldSelect<PermissionsDto>) builder,
  ) {
    var select = PermissionsDtoFieldSelect<PermissionsDto>();
    builder(select);
    return select;
  }

  PermissionsDto({required this.name, this.id = "", this.expand});

  @JsonKey(toJson: Dto.optionalStringToJson, defaultValue: "")
  @override
  final String name;
  @JsonKey(toJson: Dto.optionalStringToJson)
  @override
  final String id;
  @JsonKey(includeToJson: false)
  @override
  final PermissionsExpandDto? expand;

  factory PermissionsDto.fromRecord(RecordModel record) =>
      PermissionsDto.fromJson(record.toJson());

  factory PermissionsDto.fromJson(Map<String, dynamic> json) =>
      _$PermissionsDtoFromJson(json);

  @override
  Map<String, dynamic> toJson() => _$PermissionsDtoToJson(this);

  @override
  List<Future<http.MultipartFile>> toFiles() => const [];

  @override
  RelationDto<PermissionsDto> asRelation() => RelationDto(id);
}
